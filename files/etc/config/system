
config system
	option hostname 'SOGEKING'
	option timezone 'WIB-7'
	option ttylogin '0'
	option log_size '128'
	option urandom_seed '0'
	option zonename 'Asia/Jakarta'
	option log_proto 'udp'
	option conloglevel '4'
	option cronloglevel '9'
	option description 'To Infinity and Beyond!'

config timeserver 'ntp'
	list server '0.id.pool.ntp.org'
	list server '1.id.pool.ntp.org'
	list server '2.id.pool.ntp.org'
	list server '3.id.pool.ntp.org'

